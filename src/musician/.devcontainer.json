{
    "name": "DJ-Musician",
    "dockerComposeFile": [
        "../../docker-compose.yml"
    ],
    "postCreateCommand": "bash ../../docker/orchestra.sh",
    "service": "dj-musician",
    "shutdownAction": "none",
    "workspaceFolder": "/workspace/src/musician",
    "remoteUser": "musician",
    "extensions": [
        "donjayamanne.python-extension-pack",
        "usernamehw.errorlens"
    ],
    "settings": {
        "python.formatting.provider": "autopep8",
        "python.linting.enabled": true,
        "python.linting.pylintEnabled": true
    }
}